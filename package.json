  "license": "MIT",
  "devDependencies": {
    "cross-env": "^7.0.0",
    "laravel-mix": "^5.0.1",
    "resolve-url-loader": "3.1.0",
    "sass": "^1.25.0",
    "sass-loader": "8.*",
    "ts-loader": "^6.2.1",
